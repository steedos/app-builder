{
  "name": "@steedos/builder-charts",
  "version": "1.0.0",
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "typings": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "default": "./dist/esm/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build-rollup": "rollup -c rollup.config.ts",
    "build": "concurrently yarn:build:*",
    "build:esm": "cross-env BABEL_ENV=esm babel src --root-mode upward --extensions .ts,.tsx -d dist/esm --source-maps --copy-files",
    "build:cjs": "cross-env BABEL_ENV=cjs babel src --root-mode upward --extensions .ts,.tsx -d dist/cjs --source-maps --copy-files",
    "build:types": "tsc --emitDeclarationOnly --declaration --declarationDir dist/types",
    "start": "nodemon --watch src --exec yarn build -e ts,tsx,less",
    "test": "jest --env=jsdom --passWithNoTests",
    "lint": "concurrently yarn:lint:*",
    "version": "yarn build",
    "test:cov": "yarn test --coverage",
    "lint:src": "eslint src --ext .ts,.tsx --config ../../.eslintrc",
    "lint:types": "tsc --noEmit"
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-card": "^1.11.6",
    "@ant-design/pro-field": "^1.18.10",
    "@ant-design/pro-form": "^1.31.2",
    "@ant-design/pro-provider": "^1.4.9",
    "@ant-design/pro-skeleton": "^1.0.1",
    "@ant-design/pro-table": "^2.32.2",
    "@builder.io/react": "^1.1.42",
    "@chakra-ui/react": "^1.4.2",
    "@emotion/core": ">=10",
    "@emotion/styled": "^11.1.5",
    "@jsx-lite/core": "^0.0.17",
    "@redash/viz": "^0.1.1",
    "@rollup/plugin-alias": "^3.1.2",
    "@steedos/builder-form": "1.0.0",
    "@steedos/builder-locale": "1.0.0",
    "@steedos/builder-store": "1.0.0",
    "@steedos/filters": "^2.0.33",
    "array-move": "^3.0.1",
    "fs-extra-promise": "^1.0.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.10",
    "mobx-react-lite": "^3.2.0",
    "mobx-state-tree": "^5.0.1",
    "next": "^10.0.5",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-loadable": "^5.5.0",
    "react-query": "^3.13.3",
    "react-slick": "^0.23.2",
    "react-sortable-hoc": "^2.0.0",
    "react-to-webcomponent": "^1.5.1",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "tslib": "^2.1.0",
    "web-components-with-react": "^1.2.1",
    "webcomponents-in-react": "^1.0.1"
  },
  "devDependencies": {
    "@builder.io/block-publish": "^1.1.2",
    "@types/jest": "^26.0.19",
    "@types/lodash-es": "^4.17.1",
    "@types/node": "^14.14.19",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.0",
    "@types/react-loadable": "^5.5.1",
    "@types/react-slick": "^0.23.3",
    "colors": "^1.1.2",
    "commitizen": "4.2.2",
    "coveralls": "^3.0.0",
    "cross-env": "7.0.3",
    "cz-conventional-changelog": "3.3.0",
    "husky": "4.3.6",
    "jest": "26.6.3",
    "lint-staged": "10.5.3",
    "lodash.camelcase": "^4.3.0",
    "npm-install-peers": "^1.2.1",
    "prettier": "^2.2.1",
    "prompt": "^1.0.0",
    "replace-in-file": "^6.2.0",
    "rimraf": "3.0.2",
    "rollup": "^2.42.4",
    "rollup-plugin-alias": "^2.2.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-re": "^1.0.7",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-typescript2": "^0.30.0",
    "semantic-release": "^17.2.3",
    "ts-jest": "26.4.4",
    "ts-node": "^9.1.1",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.1.0",
    "tslint-config-standard": "^7.0.0",
    "typedoc": "^0.11.0",
    "validate-commit-msg": "^2.12.2"
  },
  "repository": "https://github.com/steedos/app-builder/tree/master/packages/builder-charts"
}
